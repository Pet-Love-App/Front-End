{
  "expo": {
    "name": "pet_love",
    "slug": "pet_love",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appIcon.png",
    "scheme": "petlove",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yuanqh23.petlove",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Pet Love 需要访问您的相机来拍摄猫粮配料表、头像和宠物照片。",
        "NSPhotoLibraryUsageDescription": "Pet Love 需要访问您的相册来选择图片用于分析配料表、设置头像或发布动态。",
        "NSMicrophoneUsageDescription": "Pet Love 需要访问麦克风来录制视频。",
        "NSLocationWhenInUseUsageDescription": "Pet Love 需要获取您的位置信息，以便在发布动态时添加位置标签。"
      }
    },
    "android": {
      "icon": "./assets/appIcon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/appIcon.png",
        "backgroundColor": "#FFE5D9"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.yuanqh23.pet_love",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO"
      ],
      "softwareKeyboardLayoutMode": "resize",
      "usesCleartextTraffic": true
    },
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "assetBundlePatterns": ["assets/**/*"],
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Pet Love 需要访问您的相机来拍摄猫粮配料表、头像和宠物照片。",
          "microphonePermission": "Pet Love 需要访问麦克风来录制视频。",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Pet Love 需要访问您的相册来选择图片用于分析配料表、设置头像或发布动态。",
          "cameraPermission": "Pet Love 需要访问您的相机来拍摄猫粮配料表、头像和宠物照片。"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Pet Love 需要获取您的位置信息，以便在发布动态时添加位置标签。",
          "locationAlwaysPermission": "Pet Love 需要获取您的位置信息，以便在发布动态时添加位置标签。",
          "locationWhenInUsePermission": "Pet Love 需要获取您的位置信息，以便在发布动态时添加位置标签。"
        }
      ],
      [
        "expo-splash-screen",
        {
          "exclude": ["**/_components/**/*", "**/*.ts", "**/*.d.ts"],
          "image": "./assets/appIcon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "tsinghua-university-dp",
          "setCommits": false
        }
      ],
      "./plugins/withNetworkSecurityConfig.js"
    ],
    "experiments": {
      "reactCompiler": true,
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "262e9319-0b93-4614-a21b-d008841a39c0"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "owner": "yuanqh233"
  }
}
