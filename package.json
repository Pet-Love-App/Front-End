{
  "name": "pet_love",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\" --ignore-path .gitignore",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "detox test --configuration android.emu.debug",
    "build:e2e": "npx expo prebuild && detox build --configuration android.emu.debug",
    "prebuild": "npx expo prebuild --clean",
    "optimize:lottie": "node scripts/optimize-lottie.js",
    "build:production": "NODE_ENV=production expo export --platform all",
    "analyze:bundle": "npx react-native-bundle-visualizer",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "arrowParens": "always",
    "endOfLine": "auto",
    "bracketSpacing": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "feat",
          "fix",
          "docs",
          "style",
          "refactor",
          "perf",
          "test",
          "chore",
          "revert",
          "build",
          "ci"
        ]
      ],
      "subject-case": [
        0
      ]
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@supabase/supabase-js": "^2.86.2",
    "@tamagui/animations-react-native": "^1.138.0",
    "@tamagui/button": "^1.138.0",
    "@tamagui/card": "^1.138.0",
    "@tamagui/config": "^1.138.0",
    "@tamagui/core": "^1.138.0",
    "@tamagui/image": "^1.138.0",
    "@tamagui/input": "^1.138.0",
    "@tamagui/lucide-icons": "^1.138.0",
    "@tamagui/portal": "^1.138.0",
    "@tamagui/scroll-view": "^1.138.0",
    "@tamagui/separator": "^1.138.0",
    "@tamagui/shapes": "^1.138.0",
    "@tamagui/sheet": "^1.138.0",
    "@tamagui/slider": "^1.138.0",
    "@tamagui/stacks": "^1.138.0",
    "@tamagui/switch": "^1.138.0",
    "@tamagui/tabs": "^1.138.0",
    "@tamagui/text": "^1.138.0",
    "base64-arraybuffer": "^1.0.2",
    "expo": "~54.0.29",
    "expo-av": "^16.0.8",
    "expo-blur": "^15.0.8",
    "expo-camera": "~17.0.10",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.11",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-image-manipulator": "~14.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.10",
    "expo-location": "~19.0.8",
    "expo-router": "~6.0.19",
    "expo-secure-store": "~15.0.8",
    "expo-sharing": "~14.0.8",
    "expo-splash-screen": "~31.0.12",
    "expo-sqlite": "~16.0.10",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-video-thumbnails": "^10.0.8",
    "expo-web-browser": "~15.0.10",
    "lottie-react-native": "~7.3.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.12.1",
    "react-native-web": "~0.21.0",
    "react-native-webview": "^13.15.0",
    "react-native-worklets": "0.5.1",
    "tamagui": "^1.138.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/detox": "^17.14.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.0",
    "@types/react-native": "^0.72.8",
    "detox": "^20.46.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "imagemin": "^9.0.1",
    "imagemin-webp": "^8.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.16",
    "jest-junit": "^16.0.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
